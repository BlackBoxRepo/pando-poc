{
    "name": "blackboxcode/pando-poc",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoBaseBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoUserBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoNoteBundle"
        },
        {
          "type": "vcs",
          "url": "https://github.com/BlackBoxRepo/PandoContactInfoBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoTicketBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoPersonBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoEmployeeBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoAccountBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoAccountTicketBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoProductSaleBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoCommissionBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoProductProviderBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoTaxBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoECommBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlackBoxRepo/PandoFileBundle"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.6.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0,>=3.0.12",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "blackboxcode/pando-base-bundle": "@dev",
        "blackboxcode/pando-user-bundle": "@dev",
        "blackboxcode/pando-note-bundle": "@dev",
        "blackboxcode/pando-contact-info-bundle": "@dev",
        "blackboxcode/pando-ticket-bundle": "@dev",
        "blackboxcode/pando-person-bundle": "@dev",
        "blackboxcode/pando-employee-bundle": "@dev",
        "blackboxcode/pando-account-bundle": "@dev",
        "blackboxcode/pando-account-ticket-bundle": "@dev",
        "blackboxcode/pando-product-sale-bundle": "@dev",
        "blackboxcode/pando-commission-bundle": "@dev",
        "blackboxcode/pando-product-provider-bundle": "@dev",
        "blackboxcode/pando-tax-bundle": "@dev",
        "blackboxcode/pando-ecomm-bundle": "@dev",
        "blackboxcode/pando-file-bundle": "@dev"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
